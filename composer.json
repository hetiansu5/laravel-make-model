{
  "name": "tinson/laravel-make-model",
  "description": "Laravel make model library",
  "type": "library",
  "keywords": [
    "laravel",
    "plugin",
    "laravel-make-model"
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "laravel/lumen-framework": "5.4.*",
    "vlucas/phpdotenv": "~2.2",
    "phpunit/phpunit": "5.7.27"
  },
  "autoload": {
    "psr-4": {
      "MakeModel\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "env-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test" : [
      "vendor/bin/phpunit --configuration phpunit.xml --colors  tests"
    ]
  }
}